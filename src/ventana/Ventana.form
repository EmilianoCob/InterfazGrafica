              <AuxValues>
                <AuxValue name="JavaCodeGenerator_InitCodePost" type="java.lang.String" value="ListSelectionListener oyenteSeleccion = new ListSelectionListener(){&#xd;&#xa;    @Override&#xd;&#xa;    public void valueChanged(ListSelectionEvent e){&#xd;&#xa;        //para quitar el doble evento&#xd;&#xa;        if(e.getValueIsAdjusting()){&#xd;&#xa;            int filaSeleccionada = tabla.getSelectedRow(); //Queremos saber la fila seleccionada, retorna un entero, filas y columnas se empiezan a contar desde 0 &#xd;&#xa;            &#xd;&#xa;            //Obtener todos los datos de la tabla que seleccionamos, estamos trabajando en el modelo de la tabla&#xd;&#xa;            String nombre = (String) modeloTabla.getValueAt(filaSeleccionada, 0); //retorna un Object, por eso se transforma a String / fila seleccionada, el otro es el numero de la columna&#xd;&#xa;            String apellidos = (String) modeloTabla.getValueAt(filaSeleccionada, 1);    &#xd;&#xa;            String email = (String) modeloTabla.getValueAt(filaSeleccionada, 2);&#xd;&#xa;            String celular = (String) modeloTabla.getValueAt(filaSeleccionada, 3);    &#xd;&#xa;            String direccion = (String) modeloTabla.getValueAt(filaSeleccionada, 4); &#xd;&#xa;&#xd;&#xa;            //Ahora solo falta mostrar los datos en cada determinada caja&#xd;&#xa;            cajaNombre.setText(nombre);&#xd;&#xa;            cajaApellidos.setText(apellidos);&#xd;&#xa;            cajaEmail.setText(email);&#xd;&#xa;            cajaCelular.setText(celular);&#xd;&#xa;            cajaDireccion.setText(direccion);&#xd;&#xa;&#xd;&#xa;        }&#xd;&#xa;    }&#xd;&#xa;};&#xd;&#xa;&#xd;&#xa;tabla.getSelectionModel().addListSelectionListener(oyenteSeleccion);&#xd;&#xa;"/>
